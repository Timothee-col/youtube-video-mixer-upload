{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": null,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  },
  "variables": {
    "PORT": "8501",
    "PYTHONUNBUFFERED": "1",
    "IS_RAILWAY": "true",
    "MAX_UPLOAD_SIZE": "500"
  }
}
